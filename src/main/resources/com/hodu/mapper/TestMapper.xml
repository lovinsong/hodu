<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hodu.mapper.TestMapper">

	<select id="getTableList" resultType="com.hodu.model.TestVO">
		SELECT * FROM TAB 
	</select>

	<select id="getTableName" resultType="com.hodu.model.TestVO">
		SELECT tname FROM TAB 
	</select>

	<select id="getTableCount" resultType="Integer">
		SELECT count(*) FROM TAB 
	</select>

</mapper>